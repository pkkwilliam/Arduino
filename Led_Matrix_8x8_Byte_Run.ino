#include <LedControl.h>#include "binary.h"LedControl lc = LedControl(12,11,10,1);byte b = B00000000;void setup() {  lc.shutdown(0,false);  lc.setIntensity(0,1);  lc.clearDisplay(0);  }void loop() {    b += 1;    lc.setRow(0,0,b);    lc.setRow(0,1,b);    lc.setRow(0,2,b);    lc.setRow(0,3,b);    lc.setRow(0,4,b);    lc.setRow(0,5,b);    lc.setRow(0,6,b);    lc.setRow(0,7,b);    delay(1000);}